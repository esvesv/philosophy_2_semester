var context={window:{location:{hash:window.location.hash,host:window.location.host,hostname:window.location.hostname,href:window.location.href,origin:window.location.origin,pathname:window.location.pathname,port:window.location.port,protocol:window.location.protocol,search:window.location.search},navigator:{language:window.navigator.language,browserLanguage:window.navigator.browserLanguage,userAgent:window.navigator.userAgent},innerHeight:window.innerHeight,innerWidth:window.innerWidth},document:{title:document.title,referrer:document.referrer}};function stringifyAndSantizeDriftFields(t){let e={};for(const n in t)n.endsWith("Id")&&(e[n]=t[n]);return JSON.stringify(e)}window.addEventListener("message",(function(t){if("https://static-site-drift-snippet.s3.amazonaws.com"!==t.origin)return;const e=document.getElementById("drift-iframe");if(null===e)return;if(!(e&&e.contentWindow||t.source!==e.contentWindow))return;const n=t.data;if("driftIframeReady"===n.type){if(window.AsanaStorage&&window.AsanaStorage.getCookie){let t=window.AsanaStorage.getCookie("_ga");null!==t&&(context.gaClientId=t.match(/GA.\..\.(.*)/i)[1]),context.isLoggedIn="true"===window.AsanaStorage.getCookie("is_logged_in")}e.contentWindow.postMessage({type:"driftSetContext",data:context},"*")}else if("driftIframeResize"===n.type){const t=n.data.styles;for(var o in t)t.hasOwnProperty(o)&&e.style.setProperty(o,t[o])}else"driftGenericGAEvent"===n.type&&window.dataLayer.push({event:"driftInteraction",eventCategory:"Drift Widget",eventAction:n.eventName,eventLabel:stringifyAndSantizeDriftFields(n.payload)})}));